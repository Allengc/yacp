  /usr/lib/python2.7/site-packages/pysearpc/client.py
  /usr/lib/python2.7/site-packages/pysearpc/client.pyc
  /usr/lib/python2.7/site-packages/pysearpc/client.pyo
  /usr/lib/python2.7/site-packages/pysearpc/common.py
  /usr/lib/python2.7/site-packages/pysearpc/common.pyc
  /usr/lib/python2.7/site-packages/pysearpc/common.pyo
  /usr/lib/python2.7/site-packages/pysearpc/server.py
  /usr/lib/python2.7/site-packages/pysearpc/server.pyc
  /usr/lib/python2.7/site-packages/pysearpc/server.pyo

