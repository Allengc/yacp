  /usr/include/searpc-utils.h
  /usr/include/searpc.h
  /usr/lib/libsearpc.dll.a
  /usr/lib/pkgconfig/libsearpc.pc

(python-libsearpc)
  /usr/bin/searpc-codegen.py
  /usr/lib/python2.7/site-packages/pysearpc/__init__.py
