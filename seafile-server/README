  libiconv2-1.14-3
  libintl-devel-0.19.8.1-2
  libjansson-devel-2.11-1bl1
  libjansson4-2.11-1bl1
  libmysqlclient18-10.1.30-1
  libopenssl100-1.0.2o-1
