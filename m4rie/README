  /usr/include/m4rie/m4ri_functions.h
  /usr/include/m4rie/m4rie.h
  /usr/include/m4rie/mzd_poly.h
  /usr/include/m4rie/mzd_slice.h
  /usr/include/m4rie/mzed.h
  /usr/include/m4rie/newton_john.h
