  /usr/include/uripp/path.h
  /usr/include/uripp/query.h
  /usr/include/uripp/scheme.h
  /usr/include/uripp/uri.h
  /usr/include/uripp/urisyn.h
  /usr/include/uripp/utils.h
  /usr/lib/liburipp.dll.a
