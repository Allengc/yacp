  /usr/lib/cabocha/model/dep.unidic.model
  /usr/lib/cabocha/model/ne.ipa.model
  /usr/lib/cabocha/model/ne.juman.model
  /usr/lib/cabocha/model/ne.unidic.model
  /usr/libexec/cabocha/cabocha-learn.exe
  /usr/libexec/cabocha/cabocha-model-index.exe
  /usr/libexec/cabocha/cabocha-system-eval.exe
