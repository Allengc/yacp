  /usr/include/lastfmlib/lastfmscrobbler.h
  /usr/include/lastfmlib/lastfmscrobblerc.h
  /usr/include/lastfmlib/lastfmtypes.h
  /usr/include/lastfmlib/nowplayinginfo.h
  /usr/include/lastfmlib/submissioninfo.h
  /usr/include/lastfmlib/submissioninfocollection.h
  /usr/include/lastfmlib/urlclient.h
