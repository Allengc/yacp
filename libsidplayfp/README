  /usr/include/sidplayfp/siddefs.h
  /usr/include/sidplayfp/sidplayfp.h
  /usr/include/sidplayfp/sidversion.h
  /usr/lib/libsidplayfp.dll.a
  /usr/lib/pkgconfig/libsidplayfp.pc

(libstilview0)
  /usr/bin/cygstilview-0.dll

(libstilview-devel)
  /usr/include/stilview/stil.h
  /usr/include/stilview/stildefs.h
  /usr/lib/libstilview.dll.a
  /usr/lib/pkgconfig/libstilview.pc

