  /usr/bin/cygsidplayfp-4.dll

(libsidplayfp-devel)
  /usr/include/sidplayfp/SidConfig.h
  /usr/include/sidplayfp/SidDatabase.h
  /usr/include/sidplayfp/SidInfo.h
  /usr/include/sidplayfp/SidTune.h
  /usr/include/sidplayfp/SidTuneInfo.h
