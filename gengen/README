  /usr/share/doc/gengen/INSTALL
  /usr/share/doc/gengen/NEWS
  /usr/share/doc/gengen/README
  /usr/share/doc/gengen/THANKS
  /usr/share/doc/gengen/gengen.html
  /usr/share/doc/gengen/index.html
  /usr/share/info/gengen.info.gz
  /usr/share/man/man1/gengen.1.gz

