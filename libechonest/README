  /usr/include/echonest/CatalogArtist.h
  /usr/include/echonest/CatalogItem.h
  /usr/include/echonest/CatalogSong.h
  /usr/include/echonest/CatalogUpdateEntry.h
  /usr/include/echonest/Config.h
