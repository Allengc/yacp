  /usr/include/libcmis-0.2/libcmis/document.hxx
  /usr/include/libcmis-0.2/libcmis/exception.hxx
  /usr/include/libcmis-0.2/libcmis/folder.hxx
  /usr/include/libcmis-0.2/libcmis/object-type.hxx
  /usr/include/libcmis-0.2/libcmis/object.hxx
  /usr/include/libcmis-0.2/libcmis/property-type.hxx
  /usr/include/libcmis-0.2/libcmis/property.hxx
