  /usr/include/libcmis-0.2/libcmis/session.hxx
  /usr/include/libcmis-0.2/libcmis/xml-utils.hxx
  /usr/include/libcmis-0.2/libcmis/xmlserializable.hxx
  /usr/lib/libcmis-0.2.dll.a
