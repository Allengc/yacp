  /usr/share/partio/test/makeline.exe
  /usr/share/partio/test/test.exe
  /usr/share/partio/test/testcache.exe
  /usr/share/partio/test/testiterator.exe
  /usr/share/partio/test/testkdtree.exe
  /usr/share/partio/test/teststr.exe

