  /usr/share/opencc/zhs2zhtw_p.ini
  /usr/share/opencc/zhs2zhtw_v.ini
  /usr/share/opencc/zhs2zhtw_vp.ini
  /usr/share/opencc/zht2zhs.ini
  /usr/share/opencc/zht2zhtw_p.ini
  /usr/share/opencc/zht2zhtw_v.ini
  /usr/share/opencc/zht2zhtw_vp.ini
  /usr/share/opencc/zhtw2zhcn_s.ini
  /usr/share/opencc/zhtw2zhcn_t.ini
  /usr/share/opencc/zhtw2zhs.ini
  /usr/share/opencc/zhtw2zht.ini

