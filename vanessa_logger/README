(libvanessa_logger0)
  /usr/bin/cygvanessa_logger-0.dll

(libvanessa_logger-devel)
  /usr/include/vanessa_logger.h
  /usr/lib/libvanessa_logger.dll.a
  /usr/lib/libvanessa_logger.la
  /usr/lib/pkgconfig/vanessa-logger.pc

