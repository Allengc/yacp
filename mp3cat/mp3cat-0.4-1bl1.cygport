DESCRIPTION="MP3 Concatenater"
HOMEPAGE="http://tomclegg.net/${PN}"
SRC_URI="http://tomclegg.net/software/${P}.tar.gz"

src_compile()
{
	cd ${B}
	lndirs
	cygmake
}

src_install()
{
	cd ${B}
	dobin mp3cat mp3log mp3log-conf mp3dirclean mp3http mp3stream-conf
	insinto /usr/share/${PN}
	doins example.*
}
