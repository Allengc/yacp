(libxc3)
  /usr/bin/cygxc-3.dll
  /usr/bin/cygxcf90-3.dll

(libxc-devel)
  /usr/include/libxc_funcs_m.mod
  /usr/include/xc.h
  /usr/include/xc_config.h
  /usr/include/xc_f90_lib_m.mod
  /usr/include/xc_f90_types_m.mod
  /usr/include/xc_funcs.h
  /usr/include/xc_unconfig.h
  /usr/include/xc_version.h
  /usr/lib/libxc.dll.a
  /usr/lib/libxcf90.dll.a
  /usr/lib/pkgconfig/libxc.pc

