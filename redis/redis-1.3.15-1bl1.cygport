DESCRIPTION="Persistent key-value database with built-in net interface written in ANSI-C for Posix systems"
HOMEPAGE="http://code.google.com/p/${PN}/"
SRC_URI="http://${PN}.googlecode.com/files/${PN}-2.0.0-rc2.tar.gz"
SRC_DIR="${PN}-2.0.0-rc2"

src_compile()
{
	cd ${B}
	lndirs
	cygmake
}

src_install()
{
	cd ${B}
	dobin *.exe
	docinto html
	dodoc doc/*
}

DIFF_EXCLUDES="release.h"
