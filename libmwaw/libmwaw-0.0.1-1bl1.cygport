DESCRIPTION="Import library for some old mac text documents"
HOMEPAGE="http://${PN}.sf.net/"
SRC_URI="mirror://sourceforge/${PN}/${P}.tar.bz2"
SRC_DIR="${PN}"

CYGCONF_ARGS="--disable-werror"

DIFF_EXCLUDES="default.apspec"

PKG_NAMES="
	${PN}
	${PN}0.0_0
	${PN}0.0-devel
"
libmwaw_CONTENTS="
	usr/bin/*.exe
	usr/share/doc/Cygwin
	usr/share/doc/${PN}/[CHR]*
"
libmwaw0_0_0_CONTENTS="
	usr/bin/*.dll
"
libmwaw0_0_devel_CONTENTS="
	usr/include
	usr/lib
"
