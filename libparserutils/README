  /usr/include/parserutils/errors.h
  /usr/include/parserutils/functypes.h
  /usr/include/parserutils/input/inputstream.h
  /usr/include/parserutils/parserutils.h
  /usr/include/parserutils/types.h
  /usr/include/parserutils/utils/buffer.h
  /usr/include/parserutils/utils/stack.h
  /usr/include/parserutils/utils/vector.h
  /usr/lib/libparserutils.dll.a
