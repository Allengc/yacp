  /usr/share/doc/snappy/framing_format.txt

(libsnappy1)
  /usr/bin/cygsnappy-1.dll

(libsnappy-devel)
  /usr/include/snappy-c.h
  /usr/include/snappy-sinksource.h
  /usr/include/snappy-stubs-public.h
  /usr/include/snappy.h
  /usr/lib/libsnappy.dll.a
