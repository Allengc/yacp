  /usr/share/colortail/conf.messages
  /usr/share/colortail/conf.secure
  /usr/share/colortail/conf.xferlog
  /usr/share/doc/Cygwin/colortail.README
  /usr/share/doc/colortail/AUTHORS
  /usr/share/doc/colortail/BUGS
  /usr/share/doc/colortail/COPYING
  /usr/share/doc/colortail/ChangeLog
  /usr/share/doc/colortail/README
  /usr/share/doc/colortail/TODO

