  /usr/include/milia/flrw.h
  /usr/include/milia/flrw_nat.h
  /usr/include/milia/metric.h
  /usr/include/milia/schechter.h
  /usr/lib/libmilia.dll.a
  /usr/lib/libmilia.la
  /usr/lib/pkgconfig/milia.pc

