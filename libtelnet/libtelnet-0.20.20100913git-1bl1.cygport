GIT_URI="git://github.com/elanthis/${PN}.git"
inherit git

DESCRIPTION="TELNET protocol handling library"
HOMEPAGE="http://github.com/elanthis/${PN}"

CYGCONF_ARGS="--disable-static"

PKG_NAMES="${PN} ${PN}2 ${PN}-devel"
libtelnet_CONTENTS="usr/bin/*.exe usr/share/doc usr/share/man/man1"
libtelnet2_CONTENTS="usr/bin/*.dll"
libtelnet_devel_CONTENTS="usr/include usr/lib usr/share/man/man3"

DIFF_EXCLUDES="doc *.m4"
