  /usr/lib/libgcab-1.0.dll.a
  /usr/lib/pkgconfig/libgcab-1.0.pc

(girepository-Gcab1.0)
  /usr/lib/girepository-1.0/GCab-1.0.typelib
  /usr/share/gir-1.0/GCab-1.0.gir

(vala-gcab)
  /usr/share/vala/vapi/libgcab-1.0.vapi

