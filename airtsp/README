(libairtsp1.01)
  /usr/bin/cygairtsp-1.01.dll

(libairtsp-devel)
  /usr/bin/airtsp-config
  /usr/include/airtsp/AIRTSP_Service.hpp
  /usr/include/airtsp/AIRTSP_Types.hpp
  /usr/lib/cmake/airtsp/airtsp-config-version.cmake
  /usr/lib/cmake/airtsp/airtsp-config.cmake
  /usr/lib/cmake/airtsp/airtsp-library-depends-relwithdebinfo.cmake
  /usr/lib/cmake/airtsp/airtsp-library-depends.cmake
  /usr/lib/libairtsp.dll.a
  /usr/lib/pkgconfig/airtsp.pc
  /usr/share/aclocal/airtsp.m4
  /usr/share/man/man1/airtsp-config.1.gz
  /usr/share/man/man3/airtsp-library.3.gz

