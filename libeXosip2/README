(libeXosip2-devel)
  /usr/include/eXosip2/eX_call.h
  /usr/include/eXosip2/eX_message.h
  /usr/include/eXosip2/eX_options.h
  /usr/include/eXosip2/eX_publish.h
  /usr/include/eXosip2/eX_refer.h
