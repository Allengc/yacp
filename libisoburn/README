  /usr/share/man/man1/xorrisofs.1.gz

(libisoburn1)
  /usr/bin/cygisoburn-1.dll

(libisoburn-devel)
  /usr/include/libisoburn/libisoburn.h
  /usr/include/libisoburn/xorriso.h
  /usr/lib/libisoburn.dll.a
  /usr/lib/pkgconfig/libisoburn-1.pc

