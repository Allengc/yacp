  /usr/share/doc/libuna/COPYING
  /usr/share/doc/libuna/ChangeLog
  /usr/share/doc/libuna/NEWS
  /usr/share/doc/libuna/README

(libuna1)
  /usr/bin/cyguna-1.dll

(libuna-devel)
  /usr/include/libuna.h
  /usr/include/libuna/definitions.h
  /usr/include/libuna/error.h
  /usr/include/libuna/extern.h
  /usr/include/libuna/features.h
  /usr/include/libuna/types.h
  /usr/lib/libuna.dll.a
  /usr/lib/pkgconfig/libuna.pc
  /usr/share/man/man3/libuna.3.gz

