DESCRIPTION="Steam Locomotive"
HOMEPAGE="http://www.tkl.iis.u-tokyo.ac.jp/~toyoda/"
SRC_URI="http://www.tkl.iis.u-tokyo.ac.jp/~toyoda/${PN}/${PN}.tar"

SRC_DIR=sl

src_compile()
{
	lndirs
	cd ${B}
	cygmake
}

src_install()
{
	cd ${B}
	dobin sl.exe
	doman sl.1
	dodoc README
}

src_test()
{
	cd ${B}
	./sl.exe
}
