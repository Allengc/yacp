  /usr/lib/cmake/rmol/rmol-library-depends-relwithdebinfo.cmake
  /usr/lib/cmake/rmol/rmol-library-depends.cmake
  /usr/lib/librmol.dll.a
  /usr/lib/pkgconfig/rmol.pc
  /usr/share/aclocal/rmol.m4
  /usr/share/man/man1/rmol-config.1.gz
  /usr/share/man/man3/rmol-library.3.gz

