  /usr/share/man/man1/rmol_drawBPC.1.gz
  /usr/share/man/man1/rmol_extractBPC.1.gz

(librmol1.00)
  /usr/bin/cygrmol-1.00.dll

(librmol-devel)
  /usr/bin/rmol-config
  /usr/include/rmol/RMOL_Service.hpp
  /usr/include/rmol/RMOL_Types.hpp
